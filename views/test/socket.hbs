<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <div class="col-6" id="approvals">
        <p>Initial message: approvals</p>
    </div>
    <script>
        eventWS.registerListener('userApprovedEvent', 'testWebSocket', (data) => {
            const messagesDiv = document.getElementById('approvals');
            const newMessage = document.createElement('p');
            newMessage.textContent = `This approved userID is ${data.userID}`;
           messagesDiv.appendChild(newMessage);
        })
        eventWS.send('userApproval', {
            status: 'active'
        })
    </script>

</body>

</html>